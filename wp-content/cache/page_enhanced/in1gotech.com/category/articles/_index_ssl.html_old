    </div><!--end post header-->
    <div class="entry clear"> 
       		<div data-elementor-type="wp-post" data-elementor-id="4567" class="elementor elementor-4567">
									<section class="elementor-section elementor-top-section elementor-element elementor-element-757c563 elementor-section-boxed elementor-section-height-default elementor-section-height-default" data-id="757c563" data-element_type="section" data-settings="{&quot;background_background&quot;:&quot;classic&quot;}">
